


    
   <section>
        <div class="container">
            <h4 class="heading">Book Your Service</h4>
          	<form action="#" method="post" class="booking-frm">
                <div class="form-group frm-col-1">
                	<label for="Name">Name</label>
                    <input id="name" type="text" placeholder="Name" class="form-control">
                </div>
                <div class="form-group frm-col-2">
                	<label for="Email">Email</label>
                    <input id="email" type="text" placeholder="Email" class="form-control">
                </div>

                <div class="form-group frm-col-2">
                	<label for="Mobile">Phone NO</label>
                    <input id="mobile" type="text" placeholder="9999999999" class="form-control">
                </div>

				<div class="form-group frm-col-1">
                	<label for="Company">Company</label>
                    <input id="Company" type="text" placeholder="Car Company Name" class="form-control">
                </div>
                <div class="form-group frm-col-2">
                	<label for="model">Car Model</label>
                    <input id="model" type="text" placeholder="Car Model" class="form-control">
                </div>

                <div class="form-group frm-col-2">
                	<label for="Mobile">Car Reg No</label>
                    <input id="reg-no" type="text" placeholder="Car reg no" class="form-control">
                </div>


                <div class="form-group frm-col-2">
                	<label for="State">State</label>
                    <select name="" id="State" class="form-control">
            			<option value=""></option>
            		</select>
                </div>

                <div class="form-group frm-col-2">
                	<label for="City">City</label>
                   <select name="" id="City" class="form-control">
            			<option value=""></option>
            		</select>
                </div>

				<div class="form-group frm-col-2">
                	<label for="Name">Service Center</label>
            		<select name="" id="" class="form-control">
            			<option value=""></option>
            		</select>
                </div>
                <div class="form-group frm-col-2">
                	<label for="Area">Area</label>
                    <select name="" id="Area" class="form-control">
            			<option value=""></option>
            		</select>
                </div>

                <div class="form-group frm-col-2">
                	<label for="Date">Appointment Date</label>
                    <input type="date" class="form-control">
                </div>

                <div class="form-group frm-col-2">
                	<label for="Time">Appointment Time</label>
                    <input type="time" class="form-control">
                </div>
                <hr>
				<div class="form-group" class="booking-btn-area">
	                <p class="text-center">
	                	<span>Pay in cash at outlet</span>
	                    <button type="button" class="btn btn-primary"> Book You Service</button>
	                </p>
	            </div>    
            </form>
        </div>
    </section>